{
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@now/node-server" }],
  "routes": [
    {
      "src": "/",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/notes",
      "dest": "routes/servnotesRouter.js",
      "methods": ["GET","POST","DELETE"]
    }
  ]
}